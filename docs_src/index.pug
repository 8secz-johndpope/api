doctype html
html(lang='en')
    head
        title= 'Entu API Documentation'

        meta(charset='utf-8')
        meta(name='viewport', content='width=device-width, initial-scale=1')

        link(href='https://fonts.googleapis.com/css?family=Roboto:100,300,400,500,700&subset=latin-ext,cyrillic-ext', rel='stylesheet', type='text/css')
        link(href='https://cdn.jsdelivr.net/npm/bootstrap@3/dist/js/bootstrap.min.css', rel='stylesheet', type='text/css')
        link(href='https://cdn.jsdelivr.net/gh/afeld/bootstrap-toc@v0/dist/bootstrap-toc.min.css', rel='stylesheet', type='text/css')
        link(href='https://cdn.jsdelivr.net/npm/highlight.js@9/styles/github.min.css', rel='stylesheet', type='text/css')
        link(href='style.css', rel='stylesheet', type='text/css')

    body(data-spy='scroll', data-target='#toc')
        .container-fluid
            .row
                .col-xs-12.col-sm-2
                    nav#toc(data-spy='affix', data-toggle='toc')
                .col-xs-12.col-sm-8
                    include:markdown-it ../README.md

        script(src='https://cdn.jsdelivr.net/gh/jquery/jquery@2/dist/jquery.min.js')
        script(src='https://cdn.jsdelivr.net/npm/bootstrap@3/dist/js/bootstrap.min.js')
        script(src='https://cdn.jsdelivr.net/gh/afeld/bootstrap-toc@v0/dist/bootstrap-toc.min.js')
        script(src='https://cdn.jsdelivr.net/npm/highlight.js@9/lib/highlight.min.js')
